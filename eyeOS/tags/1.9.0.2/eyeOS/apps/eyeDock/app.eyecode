	include_once(EYE_ROOT . '/' . APP_DIR . '/eyeDock/libs' . EYE_CODE_EXTENSION);
	eyeDock_lib_Load();
}

function eyeDock_end($params = '') {
	eyeWidgets('unserialize',$params);
	include_once(EYE_ROOT . '/' . APP_DIR . '/eyeDock/libs' . EYE_CODE_EXTENSION);
	eyeDock_lib_Remove();
