function xKill_on_clickScreen($params=null) {
	$pid = utf8_substr($params['arg0'][0],0,5);
	proc('close',array($pid));
	$js = 'delClickHandler("xKill");';
	eyeX('rawjs',array('js'=>$js));
	$js = 'document.getElementById("eyeApps").style.cursor="auto";';
	eyeX('rawjs',array('js'=>$js));
	proc('end');
}

