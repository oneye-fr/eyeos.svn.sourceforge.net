<info>
	<type>Function</type>
	<general>
		<name>updateContent</name>
		<tags>
			<tag>function</tag>
			<tag>library</tag>
			<tag>eyewidgets</tag>
		</tags>
		<shortDesc>Updates the serialized widgets' values.</shortDesc>		
	</general>	
	<changelog>
		<trace>
			<version>1.0</version>
			<desc>Created.</desc>
		</trace>
	</changelog>
	<definition>
		<functionName>updateContent</functionName>
		<syntax>eyeWidgets('updateContent', $params)</syntax>
		<desc>Updates the values of the serialized widgets in the current process.</desc>
		<params>
			<type>required</type>
			<name>params</name>
			<desc>Associative array, being the keys of each value the names of the serialized widgets to be updated with the values of each element.</desc>
		</params>
		<return>None.</return>
	</definition>
